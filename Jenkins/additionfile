pipeline {
  agent {
    docker 'python:3.9'
  }
  stages {
        // Stage to checkout the Git repository containing the script
        stage('Checkout') {
            steps {
                checkout SCM
            }
        }
  stages {
    stage('Run python function') {
      steps {
        sh 'python additionfile.py'
      }
    }
  }
  }
  post {
    success {
      echo " successful"
    }
    failed {
      echo " failed build"
    }
  }
  }
}

